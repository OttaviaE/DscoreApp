library(testthat)
library(RSelenium)
library(usethis)
use_test("basic")
context("basic")
library(RSelenium)
library(testthat)
remDr <- remoteDriver()
remDr$open(silent = TRUE)
appURL <- "http://fisppa.psy.unipd.it/DscoreApp/"
test_that("can connect to app", {
remDr$navigate(appURL)
appTitle <- remDr$getTitle()[[1]]
expect_equal(appTitle, "DscoreApp")
})
remDr$close()
